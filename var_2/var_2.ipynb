   "source": [
    "def polynomial_hash(s, table_size, base=31):\n",
    "    hash_value = 0\n",
    "    for char in str(s):\n",
    "        hash_value = (hash_value * base + ord(char)) % table_size\n",
    "    return hash_value\n",
    "\n",
    "polynomial_hash('5000000000', 100)"
   ],
